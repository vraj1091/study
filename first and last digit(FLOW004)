/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) 
{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<n; i++){
		    int a=sc.nextInt();
		    int sum=0;
		    int temp=a;
		    while(temp>=10){
            temp=temp/10;
		        
		    }
		    a=a%10;
		    sum=temp+a;
		    System.out.println(sum);
		}
		
	}
	}
